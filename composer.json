{
    "name": "zaeder/test-case-helper",
    "type": "symfony-bundle",
    "description": "Symfony PasswordEncoderBundle",
    "keywords": [
        "phpunit", "helper"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zaeder",
            "email": "zaeder@live.fr"
        }
    ],
    "require": {
        "php": ">7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">7.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Zaeder\\PhpUnit\\": "src/"
        }
    }
}
